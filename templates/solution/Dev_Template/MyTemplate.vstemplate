        <ProjectItem ReplaceParameters="true" TargetFileName="CoreServices.cs">CoreServices.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="AppSettings.cs">AppSettings.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Strings.resx">Strings.resx</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ResxLocalize.cs">ResxLocalize.cs</ProjectItem>        
      </Folder>
      <Folder Name="Styles" TargetFolderName="Styles">
        <ProjectItem ReplaceParameters="true" TargetFileName="Colors.xaml">Colors.xaml</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Colors.xaml.cs">Colors.xaml.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Controls.xaml">Controls.xaml</ProjectItem>
